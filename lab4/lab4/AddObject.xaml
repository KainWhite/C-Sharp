<Window x:Class="lab4.AddObject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Add Object" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterOwner">
    <Grid x:Name="gridWinContent">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <StackPanel x:Name="stackObjDefinition" Grid.Row="0" Margin="0, 20" Orientation="Vertical"></StackPanel>
        <StackPanel x:Name="stackActions" Grid.Row="1" Margin="20, 10" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button x:Name="btnApply" Click="btnApplyClick" IsDefault="True" Grid.Row="1" Padding="10, 5">Apply</Button>
        </StackPanel>
    </Grid>
</Window>
